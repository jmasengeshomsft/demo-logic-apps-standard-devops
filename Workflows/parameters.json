{
  "blob_auth": {
    "type": "object",
    "value": {
      "type": "Raw",
      "scheme": "Key",
      "parameter": "@appsetting('azureblob-connectionKey')"
    }
  },
  "cosmosdbAccountName": {
    "type": "String",
    "value": "@appsetting('COSMOSDB_ACCOUNT_NAME')"
  },
  "cosmosdbDatabaseName": {
    "type": "String",
    "value": "@appsetting('COSMOSDB_DATABASE_NAME')"
  },
  "cosmosdbContainerName": {
    "type": "String",
    "value": "@appsetting('COSMOSDB_CONTAINER_NAME')"
  }
}
